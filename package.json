{
  "name": "pietracker",
  "version": "1.0.0",
  "description": "Beautiful expense tracking application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start:backend": "cd backend && uvicorn main:app --host 0.0.0.0 --port 8000",
    "start:frontend": "cd frontend && npm run preview",
    "install:all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LSkevi/PieTracker.git"
  },
  "keywords": [
    "expense-tracker",
    "react",
    "fastapi",
    "typescript",
    "charts"
  ],
  "author": "LSkevi",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
